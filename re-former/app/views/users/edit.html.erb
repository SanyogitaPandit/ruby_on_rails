<h1>Edit <%= @user.username %></h1>
<%= form_for @user do |f| %>  
   <ul>
        <% @user.errors.full_messages.each do |error|%>
            <li><%= error%></li>
        <% end %>
  </ul>
  User name : <%= f.text_field :username %><br><br>
  E-mail    : <%= f.text_field :email %><br><br>
  Password  : <%= f.text_field :password %><br><br>
<%= f.submit %>
<%end%>